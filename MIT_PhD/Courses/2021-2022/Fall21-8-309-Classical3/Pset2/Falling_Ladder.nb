Cell[BoxData[
 RowBox[{
  RowBox[{"\[Lambda]F2", "[", 
   RowBox[{"\[Theta]_", ",", "\[Theta]00_"}], "]"}], ":=", 
  RowBox[{
   FractionBox["1", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"3", " ", 
        RowBox[{
         RowBox[{"Cos", "[", "\[Theta]", "]"}], "^", "2"}]}]}], ")"}], "^", 
     "2"}]], 
   RowBox[{"(", 
    RowBox[{"1", "+", 
     RowBox[{"3", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Sin", "[", "\[Theta]", "]"}], "-", 
         RowBox[{"Sin", "[", "\[Theta]00", "]"}]}], ")"}], "^", "2"}]}], "+", 
     
     RowBox[{"3", 
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{
         RowBox[{"Sin", "[", "\[Theta]0", "]"}], "^", "2"}]}], ")"}]}], "+", 
     RowBox[{
      FractionBox["2", "3"], 
      RowBox[{"Sin", "[", "\[Theta]", "]"}], 
      RowBox[{
       RowBox[{"Sin", "[", "\[Theta]00", "]"}], "^", "3"}]}]}], 
    ")"}]}]}]], "Input",
