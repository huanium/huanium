Cell[TextData[{
 "This is based on using the expression \n ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Tau]", "=", 
    RowBox[{
     FractionBox["4", "\[Pi]"], 
     RowBox[{"PolyLog", "[", 
      RowBox[{
       FractionBox["1", "2"], ",", "z"}], "]"}]}]}], TraditionalForm]],
  ExpressionUUID->"a395f6c6-b715-4f8c-ba04-86b14b7a10f1"],
 " where z is the fugacity ",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "=", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"\[Mu]", "*", 
      RowBox[{"/", "\[Tau]"}]}]]}], TraditionalForm]],ExpressionUUID->
  "cab96d13-8593-4086-96b7-0866c3eacaef"]
}], "Text",
 CellChangeTimes->{{3.7441067074373837`*^9, 3.744106823130705*^9}, {
  3.7441068843496037`*^9, 
  3.744107023235786*^9}},ExpressionUUID->"4f7b9167-8511-4ac7-9428-\
a9bb37ee7494"],

Cell[TextData[{
 "We seek a low temperature series. At ",
 Cell[BoxData[
  FormBox[
   RowBox[{"T", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "98a27398-3f6b-4da2-a835-030699f4125d"],
 " we have ",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "=", "1"}], TraditionalForm]],ExpressionUUID->
  "94c0a6c0-f04d-480f-90ee-5e850821a3b3"],
 " and then as ",
 StyleBox["T",
  FontSlant->"Italic"],
 " increases from zero, ",
 StyleBox["z",
  FontSlant->"Italic"],
 " decreases from 1 towards zero. So at low temperatures ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", 
    RowBox[{"z", "-", "1"}]}], TraditionalForm]],ExpressionUUID->
  "0b3c1632-ec97-41a6-9512-6ff5103efd7c"],
 " is a small quantity so we shall expand \[Tau] in powers of this"
}], "Text",
 CellChangeTimes->{{3.744107049883851*^9, 3.744107164135244*^9}, {
  3.744107198604883*^9, 
  3.744107273678966*^9}},ExpressionUUID->"69cac296-af05-4e98-a5d6-\
cabf5fa7cfc1"],

