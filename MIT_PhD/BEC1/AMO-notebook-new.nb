   RowBox[{"\[Omega]R_", ",", "\[Delta]_", ",", "\[CapitalGamma]_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"1", "/", 
     RowBox[{"(", 
      RowBox[{
