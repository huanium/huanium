       RowBox[{
        RowBox[{"\[Omega]R", "^", "2"}], "/", "2"}], "+", 
       RowBox[{
        RowBox[{"\[CapitalGamma]", "^", "2"}], "/", "4"}]}], ")"}]}], ")"}], 
   "*", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"\[Omega]R", "*", "\[Delta]"}], ",", " ", 
     RowBox[{"\[Omega]R", "*", 
      RowBox[{"\[CapitalGamma]", "/", "2"}]}], ",", 
     RowBox[{
      RowBox[{"\[Delta]", "^", "2"}], "+", 
      RowBox[{
       RowBox[{"\[CapitalGamma]", "^", "2"}], "/", "4"}]}]}], 
    "}"}]}]}]], "Input",
