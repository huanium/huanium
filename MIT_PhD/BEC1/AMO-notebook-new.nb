        RowBox[{"w", " ", "\[Omega]R"}]}]},
      {
       RowBox[{
        RowBox[{"-", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "1"}], "+", "w"}], ")"}], " ", "\[CapitalGamma]"}], 
