Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"\[Rho]Bloch", "[", 
    RowBox[{"u_", ",", "v_", ",", "w_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"IdentityMatrix", "[", "2", "]"}], "+", 
      RowBox[{"u", "*", 
       RowBox[{"PauliMatrix", "[", "1", "]"}]}], "+", 
      RowBox[{"v", "*", 
       RowBox[{"PauliMatrix", "[", "2", "]"}]}], "+", 
      RowBox[{"w", "*", 
       RowBox[{"PauliMatrix", "[", "3", "]"}]}]}], ")"}], "/", "2"}]}], 
  ";"}]], "Input",
